{
  "name": "v3",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "g": "bun scripts/generate-field-exports.ts && bun generate:types",
    "check": "cross-env NODE_OPTIONS=--no-deprecation next lint && tsc --noEmit",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit",
    "format:write": "cross-env NODE_OPTIONS=--no-deprecation prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "cross-env NODE_OPTIONS=--no-deprecation prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.1.3",
    "@payloadcms/db-mongodb": "^3.19.0",
    "@payloadcms/db-postgres": "^3.19.0",
    "@payloadcms/live-preview-react": "3.19.0",
    "@payloadcms/next": "3.19.0",
    "@payloadcms/payload-cloud": "3.19.0",
    "@payloadcms/plugin-form-builder": "3.19.0",
    "@payloadcms/plugin-nested-docs": "3.19.0",
    "@payloadcms/plugin-redirects": "3.19.0",
    "@payloadcms/plugin-search": "3.19.0",
    "@payloadcms/plugin-seo": "3.19.0",
    "@payloadcms/richtext-lexical": "3.19.0",
    "@payloadcms/ui": "3.19.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "ai": "^4.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "embla-carousel-fade": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.0.1",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "^15.1.5",
    "next-sitemap": "^4.2.3",
    "payload": "3.19.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.0.0-beta.10",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@typescript-eslint/typescript-estree": "^8.21.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^4.0.0-beta.10",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
