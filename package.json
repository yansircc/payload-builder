{
  "name": "v3",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "next start",
    "check": "cross-env NODE_OPTIONS=--no-deprecation next lint && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "prepare": "husky",
    "ci:check": "act push --container-architecture linux/amd64",
    "ci:list": "act push --container-architecture linux/amd64 -l",
    "ci:verbose": "act push --container-architecture linux/amd64 -v",
    "clean": "rm -rf .next",
    "analyze": "ANALYZE=true next build",
    "build:turbo": "NEXT_TURBO=1 next build",
    "dev:turbo": "NEXT_TURBO=1 next dev",
    "cache:clear": "rm -rf .next/cache",
    "preinstall": "bun clean",
    "docker:build": "sh scripts/docker-build.sh",
    "docker:build:local": "sh scripts/docker-build-local.sh",
    "generate:translations": "bun generate:types && bun run scripts/generate-translations/index.ts",
    "pw:open": "bun playwright test --ui",
    "pw:run": "bun playwright test",
    "postinstall": "patch-package",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
